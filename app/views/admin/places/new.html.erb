<!-- 管理者側寺社投稿ページのコンテナ -->
<div class="container">
  <!--行を示す要素で、内部のコンテンツを行内に配置-->
  <div class="row">
    <!-- 左側のコラム (幅: 5カラム) -->
    <div class="col-md-8">
      <!-- 寺社フォーム部分テンプレートを描画 -->
      <%= render 'layouts/place_form',
        place: @place,
        submit: "投稿",
        target_path: admin_places_path %>
    </div>
  </div>
</div>
